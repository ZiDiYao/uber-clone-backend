{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CustomerLoginRequest",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "minLength": 1
    },
    "loginType": {
      "type": "string",
      "enum": ["WECHAT", "EMAIL", "PHONE"]
    }
  },
  "required": ["code", "loginType"],
  "additionalProperties": false
}
